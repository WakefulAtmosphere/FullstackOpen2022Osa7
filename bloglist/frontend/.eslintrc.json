{
    "env": {
        "browser": true,
        "es2021": true,
	    "jest": true,
        "cypress/globals": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react", "jest", "cypress"
    ],
    "rules": {
        "react/function-component-definition": "off",
        "react/jsx-filename-extension": "off"
    }
}
